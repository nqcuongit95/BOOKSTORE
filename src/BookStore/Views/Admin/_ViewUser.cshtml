@model StaffViewModel

<div class="header">
    Chi Tiết
</div>
<div class="content">
    <div class="ui centered grid">
        <div class="row">
            <div class="five wide column">
                <p>Mã số: @Model.ID</p>
                <p>Họ Tên: @Model.FullName</p>
                <p>Ngày làm việc: @Model.DateCreate</p>
                <p>Số điện thoại: @Model.Phone</p>
                <p>Tài Khoản: @Model.UserName</p>
                <p>Quyền: @Model.Role</p>
            </div>
        </div>        
    </div>
</div>
<div class="actions">
    <div class="ui cancel button">Quay Lại</div>
</div>

<link href="~/css/TiniModal.css" rel="stylesheet" />