@model Role

<div class="header">
    Chi Tiết
</div>
<div class="content">
    <div class="ui centered grid">
        <div class="row">
            <div class="five wide column">
                <p>ID: @Model.Id</p>
            </div>
        </div>
        <div class="row">
            <div class="five wide column">
                <p>Tên: @Model.Name</p>
            </div>
        </div>
        <div class="row">
            <div class="five wide column">
                <p>Mô Tả: @Model.Description</p>
            </div>
        </div>
    </div>
</div>
<div class="actions">
    <div class="ui cancel button">Quay Lại</div>    
</div>

<link href="~/css/Admin.css" rel="stylesheet" />